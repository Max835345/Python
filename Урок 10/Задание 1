def razr(number,number2):
    total = 0
    total2 = 0
    while number > 0:
        number % 10
        number = number // 10
        total += 1
    while number2 > 0:
        number2 % 10
        number2 = number2 // 10
        total2 += 1
    mult = total*total2
    print(mult)


num = int(input())
num2 = int(input())
print(razr(num,num2))
